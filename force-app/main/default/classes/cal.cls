public class cal {

    public Integer a{set;get;}
    public Integer b{set;get;}
    public Integer res{set;get;}
    
    public void ma()
    {
        res=a+b;
    }
    
    public void ms()
    {
        res=a-b;
    }
    
    public void mm()
    {
        res=a*b;
    }
    
    public void md()
    {
        res=a/b;
    }
}